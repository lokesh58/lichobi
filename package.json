{
  "name": "lichobi",
  "version": "0.0.0",
  "type": "module",
  "description": "A discord bot named Lichobi",
  "main": "build/index.js",
  "scripts": {
    "build": "rm -rf build && tsc",
    "start": "dotenvx run -f .env -- node build/index.js",
    "build:start": "pnpm run build && pnpm run start",
    "dev": "nodemon --watch src --watch .env --ext ts,json --exec 'dotenvx run -f .env -- tsx' src/index.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier . --check",
    "format:fix": "prettier . --fix"
  },
  "engines": {
    "node": "^22.17.1",
    "pnpm": "^10.13.1"
  },
  "packageManager": "pnpm@10.13.1",
  "keywords": [],
  "author": "lokesh58",
  "license": "MIT",
  "imports": {
    "#lichobi/framework": "./build/framework/index.js",
    "#root/*": "./build/*"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.1",
    "@eslint/js": "^9.32.0",
    "@types/node": "^22.16.5",
    "eslint": "~9.20.1",
    "eslint-config-prettier": "^9.1.2",
    "globals": "^15.15.0",
    "nodemon": "^3.1.10",
    "prettier": "3.3.3",
    "tsx": "^4.20.3",
    "type-fest": "^4.41.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.48.3",
    "chalk": "^5.4.1",
    "discord.js": "^14.21.0",
    "ts-mixer": "^6.0.4",
    "zod": "^3.25.76"
  }
}
